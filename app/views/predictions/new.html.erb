<h1>Prediction Page</h1>

<%= form_for @prediction, url: { action: "create"} do |f| %>

<%= f.label :set_one_opener, "Set One Opener" %>
<%= f.text_field :set_one_opener %>

<%= f.label :set_one_closer, "Set One Closer" %>
<%= f.text_field :set_one_closer %>

<%= f.label :set_two_opener, "Set Two Opener" %>
<%= f.text_field :set_two_opener %>

<%= f.label :set_two_closer, "Set Two Closer"%>
<%= f.text_field :set_two_closer %>

<%= f.label :encore, "Encore"%>
<%= f.text_field :encore %>

<%= f.submit "Submit Predictions" %>

<% end %>
